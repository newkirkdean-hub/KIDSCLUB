<AnchorPane id="AnchorPane" fx:id="root" onKeyPressed="#keyListener" prefHeight="755.0" prefWidth="1025.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.settings.HelpVideosViewerController">
   <children>
      <VBox fx:id="editBox" alignment="TOP_CENTER" layoutX="117.0" layoutY="47.0" prefHeight="755.0" prefWidth="956.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <HBox prefHeight="102.0" prefWidth="832.0">
               <children>
                  <GridPane prefHeight="49.0" prefWidth="558.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="21.0" minHeight="1.0" prefHeight="8.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label fx:id="listOfLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="214.0" styleClass="recBoxLabel" text="List of  Help Videos">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="searchField" onKeyPressed="#searchRecord" prefHeight="25.0" prefWidth="184.0" promptText="Search" GridPane.columnIndex="1" />
                        <Label fx:id="listOfLabel1" alignment="CENTER" contentDisplay="CENTER" layoutX="40.0" layoutY="10.0" prefHeight="31.0" prefWidth="249.0" styleClass="recBoxLabel" text="Press (ESCAPE KEY) for new search" GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets left="30.0" right="30.0" />
                     </padding>
                  </GridPane>
               </children>
            </HBox>
            <TableView fx:id="gamesTable" onKeyPressed="#keyListener" onMouseClicked="#TableClicked" prefHeight="534.0" prefWidth="719.0">
              <columns>
                <TableColumn fx:id="gameNameColumn" prefWidth="206.0" text="Video Title" />
                <TableColumn fx:id="gameLocationColumn" prefWidth="332.0" text="Key Words" />
                  <TableColumn fx:id="gameDepartmentColumn" prefWidth="444.0" text="Link" />
              </columns>
            </TableView>
            <ButtonBar buttonOrder="" prefHeight="71.0" prefWidth="719.0">
              <buttons>
                  <Button fx:id="editButton" layoutX="585.0" layoutY="18.0" mnemonicParsing="false" onAction="#editButtonGo" text="Edit" />
                  <Button fx:id="newButton" mnemonicParsing="false" onAction="#newButtonGo" text="New" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonPushed" text="Cancel" />
                  <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectButtonPushed" text="Show Video" />
              </buttons>
            </ButtonBar>
            <HBox fx:id="editBox2" alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="1011.0" spacing="20.0">
               <children>
                  <TextField fx:id="fTitle" prefHeight="35.0" prefWidth="149.0" />
                  <TextField fx:id="fKeyWords" prefHeight="30.0" prefWidth="215.0" />
                  <TextField fx:id="fLink" alignment="CENTER" onMouseClicked="#openFileLocation" prefHeight="38.0" prefWidth="308.0" />
                  <Button fx:id="filePickerButton" mnemonicParsing="false" onAction="#openFileLocation" prefHeight="25.0" prefWidth="65.0" text="Files" />
                  <Button fx:id="cancelEditButton" mnemonicParsing="false" onAction="#cancelEditButtonGo" prefHeight="25.0" prefWidth="65.0" text="Cancel" />
                  <Button fx:id="saveButton1" layoutX="747.0" layoutY="30.0" mnemonicParsing="false" onAction="#saveButtonGo" prefHeight="25.0" prefWidth="65.0" text="Save" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
