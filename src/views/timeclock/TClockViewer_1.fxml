                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="bCancelButton" focusTraversable="false" mnemonicParsing="false" onAction="#resetExit" prefHeight="46.0" prefWidth="108.0" text="Exit" />
                  <Button fx:id="bAddNewButton" focusTraversable="false" mnemonicParsing="false" onAction="#addNewRecord" prefHeight="46.0" prefWidth="108.0" text="Add New" GridPane.columnIndex="1" />
                  <Button fx:id="cUpdateButton" focusTraversable="false" mnemonicParsing="false" onAction="#upDateTime" prefHeight="46.0" prefWidth="108.0" text="Update" GridPane.columnIndex="2" />
                  <Button fx:id="bReportsButton" focusTraversable="false" mnemonicParsing="false" onMouseEntered="#menuItemsButtonMouseOver" prefHeight="46.0" prefWidth="108.0" text="Reports" GridPane.columnIndex="5" />
                  <Button fx:id="bCalcButton" focusTraversable="false" mnemonicParsing="false" onAction="#calcTime" prefHeight="46.0" prefWidth="108.0" text="Calc Time" GridPane.columnIndex="3" />
                  <Button fx:id="bDeleteButton" focusTraversable="false" mnemonicParsing="false" onAction="#DeleteButtonPushed" prefHeight="46.0" prefWidth="108.0" text="Delete" GridPane.columnIndex="4" />
                  <Button fx:id="bDropMenuButton" mnemonicParsing="false" onAction="#bDropMenuButton" prefHeight="46.0" prefWidth="108.0" text="Drop Menus" GridPane.columnIndex="6" />
