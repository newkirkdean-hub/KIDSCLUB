<AnchorPane id="AnchorPane" fx:id="root" prefHeight="720.0" prefWidth="1063.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.timeclock.TClockViewerController">
   <children>
      <TableView fx:id="timeTable" editable="true" layoutX="18.0" layoutY="123.0" onMouseClicked="#getTableRow" prefHeight="679.0" prefWidth="1038.0">
        <columns>
          <TableColumn fx:id="empIDC1" editable="false" prefWidth="97.0" text="Employee ID" />
          <TableColumn fx:id="dateC2" prefWidth="101.0" text="Date" />
            <TableColumn fx:id="dayC3" prefWidth="87.0" text="Day" />
            <TableColumn fx:id="nameC4" prefWidth="204.0" text="Employee Name" />
            <TableColumn fx:id="timeINC5" editable="false" prefWidth="156.0" text="Time In" />
            <TableColumn fx:id="timeOUTC6" prefWidth="131.0" text="Time Out" />
            <TableColumn fx:id="totalTimeC7" prefWidth="107.0" text="Total" />
            <TableColumn fx:id="departC8" prefWidth="153.0" text="Department" />
        </columns>
      </TableView>
      <HBox layoutX="1.0" layoutY="-1.0" prefHeight="74.0" prefWidth="1063.0" spacing="20.0">
         <children>
            <GridPane prefHeight="67.0" prefWidth="862.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
