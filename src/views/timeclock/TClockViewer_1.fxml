      </HBox>
      <HBox layoutX="1.0" layoutY="63.0" prefHeight="54.0" prefWidth="1063.0" spacing="10.0">
         <children>
            <DatePicker fx:id="dateTF" prefHeight="34.0" prefWidth="136.0" promptText="Date" />
            <ComboBox fx:id="departmentTF" prefHeight="34.0" prefWidth="186.0" />
            <TextField fx:id="timeInTF" prefHeight="42.0" prefWidth="164.0" promptText="Time IN" />
            <TextField fx:id="timeOutTF" prefHeight="42.0" prefWidth="164.0" promptText="Time OUT" />
            <TextField fx:id="totalTime" prefHeight="34.0" prefWidth="107.0" promptText="Total Time" />
            <TextField fx:id="nameTF" onKeyPressed="#searchRecord" prefHeight="42.0" prefWidth="164.0" promptText="Employee Name" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
