<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="720.0" prefWidth="1069.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.vouchers.FXVoucherViewerController">
   <children>
      <TableView fx:id="cTable" editable="true" layoutX="18.0" layoutY="51.0" prefHeight="661.0" prefWidth="1038.0">
        <columns>
          <TableColumn fx:id="C2" prefWidth="178.0" text="Date" />
            <TableColumn fx:id="C3" prefWidth="78.0" text="Day" />
            <TableColumn fx:id="C4" prefWidth="225.0" text="Employee Name" />
            <TableColumn fx:id="C5" editable="false" prefWidth="98.0" text="Amount" />
            <TableColumn fx:id="C6" prefWidth="458.0" text="Reason" />
        </columns>
      </TableView>
      <HBox layoutX="1.0" layoutY="-1.0" prefHeight="52.0" prefWidth="905.0" spacing="20.0">
         <children>
            <Button fx:id="vMenuButton" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onMouseEntered="#menuItemsButtonMouseOver" prefHeight="36.0" prefWidth="87.0" text="Menu" />
            <TextField fx:id="searchField" onKeyPressed="#searchRecord" prefHeight="38.0" prefWidth="245.0" promptText="Search" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#exitButtonPushed" prefHeight="36.0" prefWidth="87.0" text="Exit" visible="false" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
